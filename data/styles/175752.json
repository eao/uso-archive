  "style": {
    "css": "@-moz-document /*[[W]]*/ url-prefix(\"https://www.messenger.com/t/\"), url-prefix(\"https://web.whatsapp.com/\") /**/ {\r\nbody {filter:invert(100%) hue-rotate(180deg) !important;}\r\nimg, [style^=\"background-image\"] {filter:brightness(75%) invert(100%) hue-rotate(180deg) !important;}\r\nimg *, [style^=\"background-image\"] * {filter:none!important;}\r\n}",
    "settings": [
      {
        "key": "W",
        "label": "Sites affected",
        "type": "dropdown",
        "options": [
          {
            "key": "M",
            "label": "messenger.com",
            "value": "/*[[W]]*/ url-prefix(\"https://www.messenger.com/t/\") /*",
            "default": false
          },
          {
            "key": "WA",
            "label": "web.whatsapp.com",
            "value": "/*[[W]]*/ url-prefix(\"https://web.whatsapp.com/\") /*",
            "default": false
          },
          {
            "key": "WAM",
            "label": "both",
            "value": "/*[[W]]*/",
            "default": true
          }
        ]
      }
    ]
