  "style": {
    "css": "@-moz-document domain(\"youtube.com\") {\r\n/*[[AnnotationsYesNo]]*/\r\n/*[[CardsButtonYesNo]]*/\r\n/*[[CSuggestionYesNo]]*/\r\n}",
    "settings": [
      {
        "key": "AnnotationsYesNo",
        "label": "Block annotations?",
        "type": "dropdown",
        "options": [
          {
            "key": "AnnotationsYes",
            "label": "Yes",
            "value": ".video-annotations, .annotation {\r\n\tdisplay: none !important;\r\n}",
            "default": true
          },
          {
            "key": "AnnotationsNo",
            "label": "No",
            "value": "",
            "default": false
          }
        ]
      },
      {
        "key": "CardsButtonYesNo",
        "label": "Block cards?",
        "type": "dropdown",
        "options": [
          {
            "key": "CardsButtonYes",
            "label": "Yes",
            "value": ".ytp-cards-button, .ytp-cards-teaser {\r\n\tdisplay: none !important;\r\n}",
            "default": true
          },
          {
            "key": "CardsButtonNo",
            "label": "No",
            "value": "",
            "default": false
          }
        ]
      },
      {
        "key": "CSuggestionYesNo",
        "label": "Block cross suggestions?",
        "type": "dropdown",
        "options": [
          {
            "key": "CSuggestionYes",
            "label": "Yes",
            "value": ".ytd-watch-flexy .ytp-ce-element {\r\n\tdisplay: none !important;\r\n}",
            "default": true
          },
          {
            "key": "CSuggestionNo",
            "label": "No",
            "value": "",
            "default": false
          }
        ]
      }
    ]
  }
