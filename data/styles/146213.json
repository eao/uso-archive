  "style": {
    "css": "@-moz-document domain(\"tweetdeck.twitter.com\") {\r\n.column-scroller {\r\n    overflow-y: hidden;\r\n}\r\n\r\n::-webkit-scrollbar {\r\n    opacity: 0 !important;\r\n    background-color: #222426 !important;\r\n}\r\n\r\nsection.js-column.column.column-type-home.will-animate {\r\n    width: /*[[home-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-interactions.will-animate {\r\n    width: /*[[noti-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-tweets.will-animate {\r\n    width: /*[[user-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-message.will-animate {\r\n    width: /*[[mess-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-list.will-animate {\r\n    width: /*[[list-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-customtimeline.will-animate {\r\n    width: /*[[collection-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-activity.will-animate {\r\n    width: /*[[activity-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-favorite.will-animate {\r\n    width: /*[[likes-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-mention.will-animate {\r\n    width: /*[[mentions-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-new-followers.will-animate {\r\n    width: /*[[followers-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-scheduled.will-animate {\r\n    width: /*[[scheduled-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n\r\nsection.js-column.column.column-type-search.will-animate {\r\n    width: /*[[trending-width]]*/px;\r\n    /*[[resizer]]*/\r\n}\r\n}",
