  "style": {
    "css": "@-moz-document regexp(\"https:\\\\/\\\\/www\\\\.reddit.com\\\\/r\\\\/\\\\w*\\\\/comments\\\\/\\\\w*\\\\/\\\\w*\\\\/\") {\r\n  .linklisting .res-expando-box.res-media-host-youtube,\r\n  .linklisting .res-expando-box.res-media-host-streamable,\r\n  .linklisting .res-expando-box.res-media-host-defaultVideo{\r\n    position: fixed;\r\n    top: 15px;\r\n    z-index: 500;\r\n    left: 60em;\r\n  }\r\n}"
