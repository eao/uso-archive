    "css": "@-moz-document domain(\"tumblr.com\") {\r\n    .compose-button {\r\n    border-color: /*[[composeborder]]*/;\r\n    background: /*[[composebackground]]*/;\r\n    opacity: /*[[composeopacity]]*/;\r\n    }\r\n    .compose-button:before{\r\n    color: /*[[composeicon]]*/;\r\n    }\r\n    .l-header-container--refresh {\r\n    background: linear-gradient(/*[[banner1]]*/, /*[[banner2]]*/);\r\n    opacity: /*[[opacity]]*/;\r\n    }\r\n}",
