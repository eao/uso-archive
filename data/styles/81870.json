  "style": {
    "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n@-moz-document regexp('(?!http://www\\.google.*).*(calendar).*') {\r\n\r\n  div.month-row > table.st-bg-table > tbody > tr > td {background-color:  /*[[column_1]]*/!important;}\r\n  div.month-row > table.st-bg-table > tbody > tr > td + td {background-color:  /*[[column_2]]*/!important;}\r\n  div.month-row > table.st-bg-table > tbody > tr > td + td + td {background-color:  /*[[column_3]]*/!important;}\r\n  div.month-row > table.st-bg-table > tbody > tr > td + td + td + td {background-color:  /*[[column_4]]*/!important;}\r\n  div.month-row > table.st-bg-table > tbody > tr > td + td + td + td + td {background-color:  /*[[column_5]]*/!important;}\r\n  div.month-row > table.st-bg-table > tbody > tr > td + td + td + td + td + td {background-color:  /*[[column_6]]*/!important;}\r\n  div.month-row > table.st-bg-table > tbody > tr > td + td + td + td + td + td + td {background-color:  /*[[column_7]]*/!important;}\r\n  div.month-row > table.st-bg-table > tbody > tr > td.st-bg-today {background-color:  /*[[today]]*/!important;}\r\n\r\n}",
