  "style": {
    "css": "@namespace url(http://www.w3.org/1999/xhtml);\r\n\r\n@-moz-document url-prefix(\"http://stackoverflow.com\") {\r\n    .question-summary, .stats, .vote, .votes, .status, .views {\r\n        -moz-border-radius: 5px;\r\n        -webkit-border-radius: 5px;\r\n    }\r\n    \r\n    .tagged-ignored:hover, .tagged-ignored:focus, .tagged-ignored:active {\r\n        -moz-box-shadow: 0px 0px 10px #888888;\r\n        -webkit-box-shadow: 0px 0px 10px #888888;\r\n    }\r\n    \r\n    .tagged-interesting:hover, .tagged-interesting:focus, .tagged-interesting:active {\r\n        -moz-box-shadow: 0px 0px 20px #FFFF55;\r\n        -webkit-box-shadow: 0px 0px 20px #FFFF55;\r\n    }\r\n}"
